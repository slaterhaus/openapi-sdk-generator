{
  "name": "openapi-sdk-gen",
  "version": "0.1.0",
  "description": "Generate TypeScript SDKs, Postman collections, GraphQL schemas, and cURL commands from OpenAPI specifications",
  "main": "dist/index.js",
  "bin": {
    "openapi-sdk-gen": "dist/cli/cli.js",
    "openapi-postman": "dist/cli/postman-cli.js",
    "openapi-graphql": "dist/cli/graphql-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "node dist/cli/cli.js"
  },
  "keywords": ["openapi", "sdk", "generator", "typescript"],
  "author": "Thomas James Slater",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "yaml": "^2.3.4"
  }
}